<Window x:Class="AP_PROJECT.View.Student.Student_offeredCourses"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AP_PROJECT.View.Student"
        mc:Ignorable="d"
        Title="Student_offeredCourses" Height="300" Width="300">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="217" Margin="10,10,0,0" VerticalAlignment="Top" Width="274">
            <DataGrid.Columns>
                <DataGridCheckBoxColumn Binding="{Binding }"  Header="Name"/>
                <DataGridCheckBoxColumn Binding="{Binding }"  Header="ID"/>
                <DataGridCheckBoxColumn Binding="{Binding }"  Header="Units"/>
                <DataGridCheckBoxColumn Binding="{Binding }"  Header="Prof"/>
                <DataGridCheckBoxColumn Binding="{Binding }"  Header="Time"/>
                <DataGridCheckBoxColumn Binding="{Binding }"  Header="M/G"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
