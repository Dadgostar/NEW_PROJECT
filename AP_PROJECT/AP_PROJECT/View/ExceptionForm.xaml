<Window x:Class="AP_PROJECT.View.ExceptionForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AP_PROJECT.View"
        mc:Ignorable="d"
        Title="ExceptionForm" Height="300" Width="300">
    <Grid>
        <RadioButton x:Name="examoverlapradiobutton" Content="Exam overlap" HorizontalAlignment="Left" Margin="170,141,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="overlapradiobutton" Content="Overlap" HorizontalAlignment="Left" Margin="170,125,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="prerequisiteradiobutton" Content="Prerequisite" HorizontalAlignment="Left" Margin="170,157,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="classvolumeradiobutton" Content="Class volum" HorizontalAlignment="Left" Margin="170,173,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5"/>
        <TextBox x:Name="explanation_txt" HorizontalAlignment="Left" Height="103" Margin="10,123,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="155"/>
        <Label x:Name="profnamelabel" Content="Professor name" HorizontalAlignment="Left" Margin="10,17,0,0" VerticalAlignment="Top" Width="75" FontSize="9"/>
        <Label x:Name="courselabel" Content="Course" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="75" FontSize="9"/>
        <Label x:Name="termlabe" Content="Term" HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" Width="75" FontSize="9"/>
        <Label x:Name="professor" Content="Label" HorizontalAlignment="Left" Margin="90,14,0,0" VerticalAlignment="Top" Width="86"/>
        <Label x:Name="course" Content="Label" HorizontalAlignment="Left" Margin="90,45,0,0" VerticalAlignment="Top" Width="86"/>
        <Label x:Name="term" Content="Label" HorizontalAlignment="Left" Margin="90,76,0,0" VerticalAlignment="Top" Width="86"/>

    </Grid>
</Window>
