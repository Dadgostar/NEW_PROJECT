        xmlns:local="clr-namespace:AP_PROJECT.View"
        mc:Ignorable="d"
        Title="Professor_objection_response" Height="300" Width="300">
    <Grid Background="#FFFFFCE4">
        <Label x:Name="student_name_label" Content="Student name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="91" Height="28"/>
        <Label x:Name="student_id" Content="Student ID" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="91" Height="27"/>
