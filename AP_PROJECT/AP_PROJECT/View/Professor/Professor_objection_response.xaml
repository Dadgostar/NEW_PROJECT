        <Label x:Name="student_nametxt" Content="" HorizontalAlignment="Left" Margin="127,7,0,0" VerticalAlignment="Top" Height="23" Width="134" Background="#FFA0CCE7"/>
        <Label x:Name="student_idtxt" Content="" HorizontalAlignment="Left" Margin="127,43,0,0" VerticalAlignment="Top" Height="27" Width="134" Background="#FFA0CCE7"/>
        <Label x:Name="course_nametxt" Content="" HorizontalAlignment="Left" Margin="127,79,0,0" VerticalAlignment="Top" Height="24" Width="134" Background="#FFA0CCE7"/>
        <RadioButton x:Name="accept_rariobutton" Content="Accept" HorizontalAlignment="Left" Margin="10,112,0,0" VerticalAlignment="Top" Width="74" FontSize="14"/>
        <RadioButton x:Name="reject_radiobutton" Content="Reject" HorizontalAlignment="Left" Margin="139,111,0,0" VerticalAlignment="Top" Width="65" Height="15" FontSize="14"/>
        <TextBlock x:Name="explanationtxt" HorizontalAlignment="Left" Height="71" Margin="20,147,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="241" Background="#FFA0CCE7"/>
